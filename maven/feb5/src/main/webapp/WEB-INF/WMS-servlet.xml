<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : WMS-servlet.xml
    Description:
        Imports the main ncWMS Spring beans, and a custom app-beans
-->

<beans xmlns='http://www.springframework.org/schema/beans'
	xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xsi:schemaLocation='http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd'>
	
	<!-- The main Spring beans needed for ncWMS -->
	<!-- This will generally be needed and should not be changed -->
	<import resource="ncWMS-beans.xml"/>
	
	<!-- Additional application-specific Spring beans -->
	<!-- This file should be customised for your specific webapp -->
	<import resource="app-beans.xml"/>

</beans>
